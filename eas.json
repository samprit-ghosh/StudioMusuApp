{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store",       
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"    
      }
    },
    "apk": {
      "distribution": "internal",    
      "android": {
        "buildType": "apk"           
      }
    }
  },
  "submit": {
    "production": {}
  }
}
